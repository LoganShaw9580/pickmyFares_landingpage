    <script>

        $(document).ready(function () {
            $("#totalPax").click(function () {

                $("#myDropdown_n").show();
            });
            $("#done").click(function () {
                $("#myDropdown_n").hide();
            });


        });

        $(window).scroll(function () {
            var scroll = $(window).scrollTop();

            if (scroll >= 10) {
                $(".colorlib-nav .top-menu").addClass("menufixed");
            } else {
                $(".colorlib-nav .top-menu").removeClass("menufixed");
            }
        });

        $(window).scroll(function () {
            var scroll = $(window).scrollTop();

            if (scroll >= -1) {
                $("#fixed-social").addClass("fixeddoiicon");
            } else {
                $("#fixed-social").removeClass("fixeddoiicon");
            }
        });


    </script>

    
  
    

    <script>
        $(function () {
            scrollIt("offers");
        });
        $(document).ready(function () {
            var windowWidth = $(window).width();
            if (windowWidth <= 1024) //for iPad & smaller devices
                $('.collapse').removeClass('show')
        });
        function DepartAirportFilter(Code) {
            if ($("#" + Code).is(':checked'))
                $(".dep" + Code).fadeIn(500);
            else
                $(".dep" + Code).fadeOut(1000);
        }

        function ArrivalAirportFilter(Code) {
            if ($("#" + Code).is(':checked'))
                $(".arr" + Code).fadeIn(500);
            else
                $(".arr" + Code).fadeOut(1000);
        }


    </script>
