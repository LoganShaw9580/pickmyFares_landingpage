<script>

    ValidateSearch = function () {
        if ($('#hid_origin').val() == "") {
            alert("Kindlly select departure airport from auto suggest list");
            $('#_origin').val('');
            $('#_origin').focus();
            return false;
        }
        else if ($('#hid_destination').val() == "") {
            alert("Kindlly select destination airport from auto suggest list");
            $('#_destination').val('');
            $('#_destination').focus();
            return false;
        }


    }
</script>
