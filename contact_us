    <script>
        completed = function (xhr, data) {

            if (xhr.status == 200) {

                alert("Your enquiry has been sent");
               
            }            
            else
                alert("Sorry, unable to send you enquiry");


        };

        failed = function (xhr, data) {

            alert("Sorry, unable to send you enquiry");
        };


    </script>



        

