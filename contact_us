    <script>

        $(document).ready(function () {
            $("#totalPax").click(function () {

                $("#myDropdown_n").show();
            });
            $("#done").click(function () {
                $("#myDropdown_n").hide();
            });


        });

        $(window).scroll(function () {
            var scroll = $(window).scrollTop();

            if (scroll >= 10) {
                $(".colorlib-nav .top-menu").addClass("menufixed");
            } else {
                $(".colorlib-nav .top-menu").removeClass("menufixed");
            }
        });

        $(window).scroll(function () {
            var scroll = $(window).scrollTop();

            if (scroll >= -1) {
                $("#fixed-social").addClass("fixeddoiicon");
            } else {
                $("#fixed-social").removeClass("fixeddoiicon");
            }
        });


    </script>

    
  
    

